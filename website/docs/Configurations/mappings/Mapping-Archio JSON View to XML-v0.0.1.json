{
	"@context": {
		"schema": "http:\/\/schema.org",
		"register": "Not Implemented"
	},
	"@type": "mapping",
	"@id": "https://vng.accept.commonground.nu/Mapping-Archio JSON View to XML.json",
	"id": 6,
	"name": "Archio JSON View to XML",
	"description": "",
	"version": "0.0.1",
	"reference": "https://vng.accept.commonground.nu/Mapping-Archio JSON View to XML.json",
	"mapping": {
		"@root": "view",
		"@attributes.identifier": "identifier",
		"@attributes.xsi:type": "Diagram",
		"name.@attributes.xml:lang": "nl",
		"name.#text": "name",
		"properties": "{ \"property\": [{% for key, value in properties %}{ \"@attributes\": { \"propertyDefinitionRef\": \"{{key}}\" }, \"value\": { \"@attributes\": { \"xml:lang\": \"nl\" }, \"#text\": \"{{value}}\" } }{% if not loop.last %},{% endif %}{% endfor %}] }",
		"node": "{% if node|default %}{{ executeMapping(5, node, true)|json_encode }}{%else%}[]{%endif%}",
		"connection": "{% if connection|default %}{{ executeMapping(4, connection, true)|json_encode }}{%else%}[]{%endif%}"
	},
	"unset": [],
	"cast": {
		"properties": "jsonToArray",
		"node": "jsonToArray",
		"connection": "jsonToArray"
	},
    "passThrough": false
}
